<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MauiApp2.Pages.HistoricoAcessos"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Histórico do Aluno">
    <Grid>
        <Image
            Aspect="AspectFill"
            HorizontalOptions="Center"
            Opacity="0.16"
            Source="logo_biomessenger.svg"
            VerticalOptions="Center" />
        <Frame
            Padding="20"
            BackgroundColor="White"
            HorizontalOptions="Center"
            VerticalOptions="Center">
            <StackLayout Spacing="20">
                <Label Text="Nome do Responsável:" />

                <Label Text="ALUNOS COM HISTÓRICO" />

                <Button Clicked="OnFecharButtonClicked" Text="Fechar" />
            </StackLayout>
        </Frame>
    </Grid>
</ContentPage>
